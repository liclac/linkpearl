<div class="row">
  <div class="col-md-6">
    {{chartist-chart ratio='ct-square' type='pie' options=chartOptions data=chartData}}
  </div>
  <div class="col-md-6">
    <table class="table">
      <thead>
        <tr>
          <th>Grand Company</th>
          <th>Members</th>
        </tr>
      </thead>
      <tbody>
        {{#each model.grand_companies as |company|}}
        <tr>
          <td>{{company.name}}</td>
          <td>{{company.members}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>

{{outlet}}
